{
  "name": "mfc-manager",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicospz/mfc-manager.git"
  },
  "scripts": {
    "dev": "nodemon -e ts --watch server -e .json,.ts -x ts-node -r tsconfig-paths/register --files --project server/tsconfig.json server/index.ts",
    "build:next": "next build",
    "build:server": "nest build --path server/tsconfig.json --config server/nest-cli.json --tsc",
    "build": "pnpm build:next && pnpm build:server",
    "start": "NODE_ENV=production node dist/main.js",
    "start:dev": "nest start --watch --path server/tsconfig.json --config server/nest-cli.json --tsc",
    "start:devnowatch": "nest start --path server/tsconfig.json --config server/nest-cli.json --tsc",
    "lint": "eslint ./",
    "prettier": "prettier --write ./src",
    "codegen": "graphql-codegen --config codegen.ts && prettier --write ./src/graphql-types.ts",
    "migration:generate": "npx typeorm migration:generate -d dist/data-source.js",
    "migration:run": "npx typeorm migration:run -d dist/data-source.js"
  },
  "dependencies": {
    "@apollo/client": "^3.7.5",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "apollo-server-express": "^3.11.1",
    "cheerio": "1.0.0-rc.12",
    "classnames": "^2.3.2",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.29.3",
    "deepmerge": "^4.3.0",
    "eslint-config-next": "^13.1.6",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "next": "^13.1.6",
    "node-fetch": "2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-swipeable": "^7.0.0",
    "recharts": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "set-cookie-parser": "^2.6.0",
    "sqlite3": "^5.1.4",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/client-preset": "^1.3.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@graphql-codegen/typescript-resolvers": "^2.7.13",
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@next/eslint-plugin-next": "^13.1.6",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/set-cookie-parser": "^2.4.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "prettier --write",
      "eslint"
    ],
    "**/*.{json,md,css}": [
      "prettier --write"
    ]
  }
}